        <servlet-name>Ulogin</servlet-name>
        <url-pattern>/Ulogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_dept</servlet-name>
        <url-pattern>/Add_dept</url-pattern>
