        <servlet-class>user.Add_user</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_inword</servlet-name>
        <servlet-class>user.Add_inword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addcat</servlet-name>
        <servlet-class>user.addcat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePass</servlet-name>
        <servlet-class>user.ChangePass</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dbcon</servlet-name>
        <url-pattern>/Dbcon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_Admin</servlet-name>
        <url-pattern>/Add_Admin</url-pattern>
    </servlet-mapping>
