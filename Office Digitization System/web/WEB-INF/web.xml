        <servlet-name>Add_inword</servlet-name>
        <url-pattern>/Add_inword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addcat</servlet-name>
        <url-pattern>/addcat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePass</servlet-name>
        <url-pattern>/ChangePass</url-pattern>
    </servlet-mapping>
